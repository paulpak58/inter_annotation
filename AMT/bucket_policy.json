{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "AllowVideoAccessForMTurkTask",
			"Effect": "Allow",
			"Principal": "*",
			"Action": "s3:GetObject",
			"Resource": "arn:aws:s3:::surgical-phase-annotation-bucket-test/test_vid.mp4",
			"Condition": {
				"StringLike": {
					"aws:Referer": [
						"https://workersandbox.mturk.com/*",
						"https://requestersandbox.mturk.com/*"
					]
				}
			}
		}
	]
}